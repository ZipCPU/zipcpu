+			"\t\t\t(and:SI (match_dup 0) (match_operand:SI 1 \"zip_opb_single_operand_p\" \"rO\"))\n"
+			"\t\t\t(match_dup 0)))]\n"
+	"\t\"\"\n"
+	"\t\"AND.%s\t%%1,%%0\t; cand\"\n"
+	"\t[(set_attr \"predicable\" \"no\") (set_attr \"ccresult\" \"unchanged\")])\n",
