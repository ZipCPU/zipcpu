+	case	LE: fprintf(stderr, "LE[!]"); break;
+	case	GTU: fprintf(stderr, "GTU[!]"); break;
+	case	LEU: fprintf(stderr, "LEU[!]"); break;
+	default:
+		fprintf(stderr, "%d", ccode); break;
+	}
