+			"\t\t\t(ior:SI (match_dup 0) (match_operand:SI 1 \"zip_opb_single_operand_p\" \"rO\"))\n"
+			"\t\t\t(match_dup 0)))]\n"
+	"\t\"\"\n"
+	"\t\"OR.%s\t%%1,%%0\t; cior\"\n"
+	"\t[(set_attr \"predicable\" \"no\") (set_attr \"ccresult\" \"unchanged\")])\n",
+	md_opname, md_cond, md_cond, zip_cond);
+}
