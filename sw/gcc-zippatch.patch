+	case PARALLEL:
+		zip_debug_print(pfx, lvl, "(PARALLEL");
+		if (XVEC(x,0) != NULL)
+			for(int j=0; j<XVECLEN(x,0);j++)
+				zip_debug_rtx_1(pfx, XVECEXP(x,0,j), lvl+1);
+		zip_debug_print(pfx, lvl, ")");
+		debug_rtx(x);
+		break;
+	case INT_LIST: zip_debug_print(pfx, lvl, "(INT-LIST"); break;
+	case SEQUENCE:
