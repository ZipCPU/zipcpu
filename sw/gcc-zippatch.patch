+; }}}
+;	
+; Match:
+;	MOV R1,R0
+;	ADD $x,R0
+;	(CCREG is dead, and x is within range ...)
+; Transform to:
+;	MOV $x(R1),R0
+; {{{
+(define_peephole2
+	[(set (match_operand:SI 0 "register_operand")
