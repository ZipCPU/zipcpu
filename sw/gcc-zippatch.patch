+static void
+zip_debug_print(const char *pfx, int lvl, const char *str) {
+	int	i;
+	i = lvl;
+	if ((true)||(lvl == 0))
+		fprintf(stderr, "%s", pfx);
+	else
+		i += strlen(pfx);
+	while(i-->0)
+		fprintf(stderr, "  ");
+	fprintf(stderr, "%s\n", str);
+}
+
+static void
+zip_debug_print_m(const char *pfx, int lvl, const char *str, enum machine_mode m) {
+	int	i;
+
+	i = lvl;
+	if ((true)||(lvl == 0))
+		fprintf(stderr, "%s", pfx);
+	else
+		i = lvl+strlen(pfx);
+	while(i-->0)
+		fprintf(stderr, "  ");
+	switch(m) {
+		case VOIDmode:
+			fprintf(stderr, "%s:V\n", str);
+			break;
+		case BLKmode:
+			fprintf(stderr, "%s:BLK\n", str);
+			break;
+		case BImode:
+			fprintf(stderr, "%s:BI\n", str);
+			break;
+		case QImode:
+			fprintf(stderr, "%s:QI\n", str);
+			break;
+		case HImode:
+			fprintf(stderr, "%s:HI\n", str);
+			break;
+#ifdef	HAVE_SImode
+		case SImode:
+			fprintf(stderr, "%s:SI\n", str);
+			break;
+#endif
+#ifdef	HAVE_DImode
+		case DImode:
+			fprintf(stderr, "%s:DI\n", str);
+			break;
+#endif
+		case CCmode:
+			fprintf(stderr, "%s:CC\n", str);
+			break;
+		default:
+			fprintf(stderr, "%s:?\n", str);
+	}
+}
