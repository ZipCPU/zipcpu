+	"MOV\tuR0,%1	; zip_save_context
+	MOV\tuR1,%2
+	MOV\tuR2,%3
+	MOV\tuR3,%4
+	SW\t%1,%0
+	SW\t%2,4(%0)
+	SW\t%3,8(%0)
+	SW\t%4,12(%0)
+	MOV\tuR4,%1
+	MOV\tuR5,%2
+	MOV\tuR6,%3
+	MOV\tuR7,%4
+	SW\t%1,16(%0)
+	SW\t%2,20(%0)
+	SW\t%3,24(%0)
+	SW\t%4,28(%0)
