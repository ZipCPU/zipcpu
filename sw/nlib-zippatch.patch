+void
+_outbyte(char v) {
+#ifdef	_ZIP_HAS_WBUART
+	if (v == '\n') {
+		// Depend upon the WBUART, not the PIC
+		while((_uart->u_fifo & 0x010000)==0)
+			;
+		UARTTX = (unsigned)'\r';
+	}
+
+	// Depend upon the WBUART, not the PIC
+	while((_uart->u_fifo & 0x010000)==0)
+		;
+	uint8_t c = v;
+	UARTTX = (unsigned)c;
+#else
+#ifdef	_ZIP_HAS_UARTTX
+	// Depend upon the WBUART, not the PIC
+	while(UARTTX & 0x100)
+		;
+	uint8_t c = v;
+	UARTTX = (unsigned)c;
+#endif
+#endif
+}
