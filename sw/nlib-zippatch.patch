+
+
+syscalls.o: $(srcdir)/syscalls.c $(srcdir)/boards/zipbasic.h $(srcdir)/zipcpu.h
+syscalls.o: $(srcdir)/zipsys.h
+udiv.o: $(srcdir)/udiv.h
